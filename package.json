{
  "name": "github-repo-search",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:api build:front",
    "build:api": "warp build api/",
    "build:front": "webpack --progress --mode=production",
    "deploy": "warp deploy --asset-dir=dist api/",
    "dev": "run-p dev:*",
    "dev:api": "warp start-emulator --watch api/",
    "dev:front": "webpack-dev-server --open",
    "postinstall": "cd api/ && npm install"
  },
  "dependencies": {
    "@warpjs/engine": "3.0.10",
    "date-fns": "^2.16.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.3.0",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "warp": "3.0.10",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
